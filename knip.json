{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "scripts/**/*.ts",
    "worker/**/*.ts",
    "server/**/*.ts",
    "utils/**/*.ts",
    "terraform/main.ts"
  ],
  "project": [
    "app/**/*.{ts,tsx,js,jsx}",
    "components/**/*.{ts,tsx,js,jsx}",
    "scripts/**/*.{ts,tsx,js,jsx}",
    "worker/**/*.{ts,tsx,js,jsx}",
    "server/**/*.{ts,tsx,js,jsx}",
    "utils/**/*.{ts,tsx,js,jsx}",
    "terraform/main.ts",
    "!**/*.test.{ts,tsx,js,jsx}",
    "!**/*.spec.{ts,tsx,js,jsx}",
    "!eslint-rules/**/*.js"
  ],
  "ignore": [
    "dist/**",
    "node_modules/**",
    "**/*.test.{ts,tsx,js,jsx}",
    "**/*.spec.{ts,tsx,js,jsx}",
    "eslint-rules/**/*.js",
    "drizzle.config.ts",
    "next.config.ts",
    "tailwind.config.ts",
    "vitest.config.ts",
    "knip.config.json",
    "terraform/lambda/lambda-email-processor.js",
    "utils/format-zod-error.ts",
    "utils/error-response.ts",
    "db/connection.ts",
    "scripts/utils.ts",
    "worker/database.ts",
    "server/versioning.ts",
    "server/index.ts",
    "app/actions.ts",
    "app/lib/webauthn.ts",
    "app/lib/pdf-generator.ts",
    "app/lib/s3-client.ts",
    "app/lib/vellum-server-utils.ts",
    "worker/runner.ts",
    "worker/agent-server.ts"
  ],
  "ignoreDependencies": [
    "@stripe/stripe-js",
    "cors",
    "helmet",
    "twilio",
    "cdktf-cli",
    "constructs",
    "eslint",
    "eslint-config-next",
    "madge",
    "postcss-load-config"
  ],
  "ignoreBinaries": ["prettier"]
}
